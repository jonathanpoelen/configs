"echo cycle pause > ~/rawdisk/music-control"
	Mod4 + space

"echo playlist_next > ~/rawdisk/music-control"
        Mod4 + Right

"echo playlist_prev > ~/rawdisk/music-control"
        Mod4 + Left

"echo seek 5 > ~/rawdisk/music-control"
        Mod4 + Up 

"echo seek -5 > ~/rawdisk/music-control"
        Mod4 + Down 

"echo cycle loop-file > ~/rawdisk/music-control"
        Mod4 + l

"readlink /proc/"$(ps -o pid,command -C mpv | grep music-control | (read pid o && echo $pid))"/fd/* | grep /home/jonathan/Music >> ~/track_passed.txt ; echo playlist_next > ~/rawdisk/music-control"
	Mod4 + u

