:set editor=kwrite
:set autocomplete=false
:colorscheme seaweed

nnoremap j      8j
nnoremap k      8k
nnoremap J      16j
nnoremap K      16k
nnoremap <A-j>  32j
nnoremap <A-k>  32k
nnoremap ø      32j
nnoremap œ      32k

nnoremap <A-space> <C-b>
nnoremap <C-space> <C-b>
nnoremap <C-A-space> <C-b>

:nmap <C-[> :tabh back<CR>
:nmap <C-]> :tabh forward<CR>
:nmap <C-6> :tabh swap<CR>
:nmap ? gnogndgnz2gT
:nmap <C-Y> :set! browser.link.open_newwindow=1<CR>:set! browser.link.open_newwindow.restriction=1<CR>

:nmap <C-j> :js noscriptOverlay.showUI()<CR>

nnoremap ` :js statusline.setVisibility(statusline.setVisibility.TOGGLE)<CR>
:js statusline.setVisibility(statusline.setVisibility.TOGGLE);
:set fullscreen=true

:js document.getElementById("liberator-status-location").style.width="9999px"
:js document.getElementById("liberator-status-location").style['border-left']="1px solid green"
:js document.getElementById("liberator-status-location").style['padding-left']="1em"

:js services.services.autoCompleteSearch.class_="@mozilla.org/autocomplete/search;1?name=unifiedcomplete"

:command -nargs=0 dlpg :execute "!xterm -geometry 222+260+0 -e '/home/jonathan/game/dl-url '\\''" + content.location.href.replace("'", "'\\''") + "'\\'''&"
:nmap Y :dlpg<CR><ESC>
