:set editor=kwrite
:set autocomplete=false
:colorscheme seaweed

:nmap <C-[> :tabh back<CR>
:nmap <C-]> :tabh forward<CR>
:nmap <C-6> :tabh swap<CR>
:nmap ? gnogndgnz2gT
:nmap <C-Y> :set! browser.link.open_newwindow=1<CR>:set! browser.link.open_newwindow.restriction=1<CR>

nnoremap j      8j
nnoremap k      8k
nnoremap J      16j
nnoremap K      16k
nnoremap <A-j>  32j
nnoremap <A-k>  32k
nnoremap é      g$
nnoremap á      g0
nnoremap <C-e>  g$
nnoremap <C-a>  g0

:js statusline.setVisibility(statusline.setVisibility.TOGGLE);
:set fullscreen=true

:js document.getElementById("liberator-status-location").style.width="9999px"
:js document.getElementById("liberator-status-location").style['border-left']="1px solid green"
:js document.getElementById("liberator-status-location").style['padding-left']="1em"
